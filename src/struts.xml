<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="default" extends="struts-default,json-default"
		namespace="/">

		<action name="ListDownloads"
			class="org.jdownloader.extensions.webinterfaceng.actions.ListDownloads">
			<result name="success" type="json" >
			
			</result>
		</action>

	</package>

	<include file="config-browser.xml" />

	<constant name="struts.devMode" value="true" />
	<constant name="struts.configuration.xml.reload" value="true" />
	<constant name="struts.convention.default.parent.package"
		value="default" />
	<constant name="struts.convention.classes.reload" value="true" />
	<constant name="struts.convention.action.disableScanning"
		value="false" />
	<constant name="struts.convention.action.packages"
		value="org.jdownloader.extensions.webinterfaceng.actions" />

	<!-- <constant name="struts.custom.i18n.resources" value="globalMessages"/> -->
	<!-- <constant name="struts.convention.result.path" value="/WEB-INF/content" 
		/> src/org/jdownloader/extensions/webinterfaceng/WebContent -->

</struts>